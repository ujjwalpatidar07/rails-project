<div class="row">
  
  
  <div class="col">
    <h3>Registered User Subscription</h3>
    <table class="table table-dark table-hover">
      <tr>
        <th>User Name</th>
        <th>Plan Name</th>
        <th>Plan Type</th>
        <th>Plan Value</th>
      </tr>
      <% @subscriptions.each do |subscription| %>
        <tr>
          <td><%=  %></td>
          <td><%= subscription.subscription_plan.plan_name %></td>
          <td><%=  subscription.subscription_plan.plan_type %></td>
          <td><%=  subscription.subscription_plan.plan_value %></td>
        </tr>
      <% end %>
    </table>
    <%= link_to 'Add New Subscription', new_subscription_path %><br>
    <%= link_to "Back", root_path %>
  </div>
 
</div>
